---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xhtml="http://www.w3.org/1999/xhtml">
	{%- for page in site.html_pages -%}
	{%- unless page.layout != 'default' or page.layout != 'article' and page.url contains '.html' -%}
	{%- unless page.url contains "/teapot" -%}
	<url>
		<loc>{{- page.url | absolute_url -}}</loc>
		<lastmod>{{ page.date | default: site.time | date: "%Y-%m-%d" }}</lastmod>
		<changefreq>weekly</changefreq>
		<priority>0.8</priority>
	</url>
	{%- endunless -%}
	{%- endunless -%}
	{%- endfor -%}
	{%- for post in site.posts -%}
	<url>
		<loc>{{- post.url | absolute_url -}}</loc>
		<lastmod>{{ post.date | default: site.time | date: "%Y-%m-%d" }}</lastmod>
		<changefreq>yearly</changefreq>
		<priority>0.5</priority>
	</url>
	{%- endfor -%}
</urlset>